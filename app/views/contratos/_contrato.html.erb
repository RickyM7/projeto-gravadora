<div id="<%= dom_id contrato %>">
  <p>
    <strong>Data de início:</strong>
    <%= contrato.data_inicio.strftime('%d/%m/%Y') %>
  </p>

  <p>
    <strong>Data de finalização:</strong>
    <%= contrato.data_fim.strftime('%d/%m/%Y') %>
  </p>

  <p>
    <strong>Cláusulas:</strong>
    <%= contrato.clausulas %>
  </p>

  <p>
    <strong>Valor:</strong>
    <%= contrato.valor %>
  </p>

  <p>
    <strong>Artista:</strong>
    <%= contrato.artista_id %>
  </p>

</div>
